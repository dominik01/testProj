            };
            s.parentNode.insertBefore(tk, s)
        })(document);</script>
    <script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyBkypr9PzKiUoZKLUF29glEu0zzGK-gE_0&callback=initMap"></script>
    <script>var myLatlng = new google.maps.LatLng(48.150065, 17.115720);
        var mapDiv = document.getElementById('map-canvas');
        var map = new google.maps.Map(mapDiv, {
            center: myLatlng,
            zoom: 16
        });
        var marker = new google.maps.Marker({
            position: myLatlng,
            title: "Satori stage"
        });
        marker.setMap(map);</script>
{% endblock %}
</body>
</html>
